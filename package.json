{
  "name": "yurinet-next",
  "productName": "YuriNET 2",
  "version": "1.0.0",
  "description": "",
  "main": "main/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=development electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir"
  },
  "build": {
    "appId": "com.thaira2.autoupdate",
    "productName": "YuriNET 2",
    "win": {
      "icon": "icons/icon.ico",
      "target": [
        "squirrel"
      ]
    },
    "squirrelWindows": {
      "iconUrl": ""
    },
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "axios": "0.18.0",
    "electron": "3.0.7",
    "electron-builder": "20.29.0",
    "next": "latest",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "next-compose-plugins": "2.1.1",
    "next-redux-wrapper": "latest",
    "next-routes": "1.4.2",
    "node-sass": "4.9.4",
    "prop-types": "15.6.2",
    "react-jss": "8.6.1",
    "react-redux": "5.1.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.5",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "4.0.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-import": "1.11.0",
    "babel-plugin-styled-components": "1.8.0",
    "cross-env": "5.2.0",
    "eslint": "5.8.0",
    "eslint-plugin-react": "7.11.1",
    "electron-builder-squirrel-windows": "^20.36.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "dependencies": {
    "electron-is-dev": "1.0.1",
    "electron-next": "3.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icharge/yurinet-next.git"
  },
  "keywords": [
    "YuriNET",
    "Online",
    "Game",
    "CNC",
    "Westwood",
    "Red Alert 2",
    "Yuri's Revenge"
  ],
  "author": "Norrapat Nimmanee <ThaiRA2Lovers Admin>",
  "bugs": {
    "url": "https://github.com/icharge/yurinet-next/issues"
  },
  "homepage": "https://github.com/icharge/yurinet-next#readme"
}
