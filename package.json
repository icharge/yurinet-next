{
  "name": "yurinet-next",
  "productName": "YuriNET 2",
  "version": "2.0.0",
  "description": "YuriNET, A Yuri's Revenge client helper",
  "main": "main/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=development electron .",
    "build": "next build renderer && next export renderer",
    "pack": "build --dir",
    "pack-win": "build",
    "pack-win-debug": "cross-env DEBUG=electron-builder build",
    "dist": "npm run build && npm run pack-win"
  },
  "build": {
    "appId": "com.thaira2.yurinet2",
    "productName": "YuriNET 2",
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ],
    "win": {
      "icon": "icons/icon.ico",
      "target": [
        "squirrel"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/matiastucci/electron-auto-updater-example/master/icons/icon.ico"
    },
    "asar": true,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "@zeit/next-sass": "^1.0.1",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-styled-components": "1.8.0",
    "electron": "3.0.7",
    "electron-builder": "20.36.0",
    "electron-builder-squirrel-windows": "^20.36.0",
    "eslint": "5.8.0",
    "eslint-plugin-react": "7.11.1",
    "immutable": "4.0.0-rc.12",
    "isomorphic-unfetch": "3.0.0",
    "jss": "9.8.7",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "next": "latest",
    "next-compose-plugins": "2.1.1",
    "next-redux-wrapper": "latest",
    "next-routes": "1.4.2",
    "node-sass": "4.9.4",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-jss": "8.6.1",
    "react-redux": "5.1.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.5",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "reset-css": "4.0.1",
    "styled-components": "4.0.3"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "electron-is-dev": "1.0.1",
    "electron-next": "3.1.5",
    "electron-squirrel-startup": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icharge/yurinet-next.git"
  },
  "keywords": [
    "YuriNET",
    "Online",
    "Game",
    "CNC",
    "Westwood",
    "Red Alert 2",
    "Yuri's Revenge"
  ],
  "author": "Norrapat Nimmanee <ThaiRA2Lovers Admin>",
  "bugs": {
    "url": "https://github.com/icharge/yurinet-next/issues"
  },
  "homepage": "https://github.com/icharge/yurinet-next#readme"
}